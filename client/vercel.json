{
  "rewrites": [
    {
      "source": "/set-password/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|_next|_static|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}
